:Cresus a :Man .

@forAll :X.
{
	:X a :Man .	# indispensable to bind ?X before applying e:disjunction
	(
		{:X :possess :gold}
		{:X :possess :diamond}
	) ! e:disjunction
} => {
	:X :possess :happiness } .
